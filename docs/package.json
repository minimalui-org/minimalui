{
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://minimalui-org.github.io/minimalui",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "deploy": "gh-pages -d web-build -r git@github.com:minimalui-org/minimalui.git",
    "predeploy": "expo build:web",
    "cp-package": "cp -R ../packages/minimalui/dist ./node_modules/@minimalui/core",
    "watch": "yarn run cp-package && onchange '../packages/minimalui/dist/**/*.js' -- yarn run cp-package"
  },
  "dependencies": {
    "@expo-google-fonts/inconsolata": "^0.1.0",
    "@expo-google-fonts/noto-sans": "^0.1.0",
    "@expo/vector-icons": "^12.0.0",
    "@minimalui/core": "^0.0.11",
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "^5.12.3",
    "@react-navigation/native": "^5.9.2",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-web": "~0.13.12",
    "react-syntax-highlighter": "^15.4.3"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^3.1.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "typescript": "~4.0.0"
  },
  "private": true
}
